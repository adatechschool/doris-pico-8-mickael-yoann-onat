function shoot()
	local new_ball={
		x=p.x*8,
		y=p.y*8,
		speed=p.ballspeed,
		portee=p.portee
	}
	add(sorts,new_ball)
end

--t={portee=20}

function update_sorts()
	for s in all(sorts) do
		local position_init=p.x*8
		s.x+=p.ballspeed
	 if (s.portee>0 and s.x>(position_init+s.portee)) del (sorts,s)
	 if (s.portee<0 and s.x<(position_init+s.portee)) del (sorts,s)
	end
end
