function spawn_enemies(amount)
gap= (128-8*amount)/(amount+1)
for i=1,amount do
 new_enemy={
  x=gap*i+8*(i-1),
  y=15,
    
 life=4
 }
add(enemies,new_enemy)
 end

end

function update_enemies()
for e in all(enemies) do
//if e.y< 60 then
if p.x>e.x then
	e.x+=0.5/8
elseif p.x<e.x then
 e.x-=0.5/8
end

if p.y>e.y then
	e.y+=0.5/8
elseif p.y<e.y then
 e.y-=0.5/8
end

end
end
